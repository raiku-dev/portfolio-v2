{
  "extends": "astro/tsconfigs/strictest",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components": [
        "src/components/*"
      ],
      "@layouts": [
        "src/layouts"
      ],
      "@assets": [
        "src/assets"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}